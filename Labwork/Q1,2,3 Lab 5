#include <iostream>
using namespace std;
int main()
{ 
    string password;
    bool incorrectpassword = true;
    
    do{
        cout << "Enter password : ";
        cin >> password; 
        if (password=="Python123"){
            cout<<"account unlocked"<<endl;
            break;
        }
        cout<<endl<<"password incorrect"<<endl<<endl;
            
    }while (true);
    return 0;
}

Q2
#include <iostream>
using namespace std;
int main()
{
    int number;
    cout<< "Enter a number :";
    cin>> number;
    for(int i=2; i<=number/2; i++)
    {
        if (number/i==0)
        cout<< "Number is not a prime number"<< endl;
        else
        cout<< "Number is a prime number"<< endl;
    }


    return 0;
}

