#include <iostream>
using namespace std;
int main (){

    int array[2][2]{{4,2},{6,1}};
    int array2[2][2]{{2,2},{3,1}};
    for(int i = 0;i < 2; i++){
        for(int j = 0;j < 2;j++){
            cout << array[i][j] << " ";
        }
       cout << endl; 
    }
    cout << endl;
    for(int i = 0;i < 2; i++){
        for(int j = 0;j < 2;j++){
            cout << array2[i][j] << " ";
            
        }
        cout << endl;
    }
    int newarray[2][2]{0};
    for(int i = 0; i < 2;i++){
        for(int j = 0; j <  2; j++){
            for(int k = 0;k < 2; k++){
                newarray[i][j] += array[i][k] * array2[k][j];
            }
            
        }
       
    }
    for (int i = 0; i < 2; i++) {
        for (int j = 0; j < 2; j++) {
            cout << newarray[i][j] << "  ";
            cout << endl;
        }
    return 0;
}
}
