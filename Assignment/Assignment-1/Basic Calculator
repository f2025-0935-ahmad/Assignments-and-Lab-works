#include <iostream>
using namespace std;

int main()
{   double x,y;
    cout << "Enter first number:" << endl;
    cin >> x;

    cout << "Enter second number:" << endl;
    cin >> y;

    cout << "resultsL:" << endl;
    cout << "sum =" << x + y <<endl;
    cout << "difference =" << x-y <<endl;
    cout << "product =" << x*y <<endl;
    cout << "quiotient =" << x/y <<endl;


return 0;
}
